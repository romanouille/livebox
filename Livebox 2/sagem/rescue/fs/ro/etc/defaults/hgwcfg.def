<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<tns:hgwconfig xmlns:tns="http://xml.softathome.org/2009/hgwconfig" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xml.softathome.org/2009/hgwconfig">
   <tns:Networking>
      <tns:WANConnection>
         <tns:WanMode>DSL_PPP</tns:WanMode>
      </tns:WANConnection>
      <tns:LANConnection>
         <tns:address>192.168.1.1</tns:address>
         <tns:netmask>255.255.255.0</tns:netmask>
      </tns:LANConnection>
      <tns:DHCP>
         <tns:startaddress>192.168.1.10</tns:startaddress>
         <tns:endaddress>192.168.1.150</tns:endaddress>
         <tns:DHCPEnable>true</tns:DHCPEnable>
      </tns:DHCP>
      <tns:DHCPv6>
	 <tns:DHCPv6Enable>true</tns:DHCPv6Enable>
      </tns:DHCPv6>
      <tns:WifiScheduling>
         <tns:AlwaysAllow>true</tns:AlwaysAllow>
      </tns:WifiScheduling>
      <tns:Firewall>      
         <tns:FirewallLevel>Medium</tns:FirewallLevel>
         <tns:FirewallRespondToPing>false</tns:FirewallRespondToPing>
         <tns:FirewallRule>
            <tns:Service>HTTP</tns:Service>
            <tns:Protocol>TCP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>80-80</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>HTTPS</tns:Service>
            <tns:Protocol>TCP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>443-443</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>POP3</tns:Service>
            <tns:Protocol>TCP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>110-110</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>POP3S</tns:Service>
            <tns:Protocol>TCP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>995-995</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>SMTPAuth</tns:Service>
            <tns:Protocol>TCP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>587-587</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>SMTP</tns:Service>
            <tns:Protocol>TCP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>25-25</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>FTP</tns:Service>
            <tns:Protocol>BOTH</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>20-21</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>SSH</tns:Service>
            <tns:Protocol>TCP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>22-22</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>NTP</tns:Service>
            <tns:Protocol>UDP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>123-123</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>NNTP</tns:Service>
            <tns:Protocol>TCP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>119-119</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>NNTPS</tns:Service>
            <tns:Protocol>TCP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>563-563</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>DNS</tns:Service>
            <tns:Protocol>BOTH</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>53-53</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>IRC</tns:Service>
            <tns:Protocol>TCP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>6666-6667</tns:DestinationPort>
            <tns:Action>reject</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>IMAP</tns:Service>
            <tns:Protocol>TCP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>143-143</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>IMAPS</tns:Service>
            <tns:Protocol>TCP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>993-993</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>ISAKMP</tns:Service>
            <tns:Protocol>UDP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>500-500</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>STUN</tns:Service>
            <tns:Protocol>UDP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>3478-3478</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>IPSEC-NAT-T</tns:Service>
            <tns:Protocol>UDP</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort>4500-4500</tns:DestinationPort>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
         <tns:FirewallRule>
            <tns:Service>ICMP</tns:Service>
            <tns:Protocol>1</tns:Protocol>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort/>
            <tns:Action>accept</tns:Action>
            <tns:Enable>true</tns:Enable>
         </tns:FirewallRule>
      </tns:Firewall>
   </tns:Networking>
   <tns:FTSettings>
       <tns:FTEnableEZP>true</tns:FTEnableEZP>
   </tns:FTSettings>
   <tns:AdminSettings>
      <tns:Language>fr-FR</tns:Language>
      <tns:AdminPassword>$(ADMIN_DEFAULT_PASSWORD)</tns:AdminPassword>
      <tns:AdminTimeout>10</tns:AdminTimeout>
      <tns:AdminConfirmationPopups>true</tns:AdminConfirmationPopups>
      <tns:UPnPIGDEnable>true</tns:UPnPIGDEnable>
      <tns:SambaEnable>true</tns:SambaEnable>
      <tns:NTP>
        <tns:NTPServer1>thotfr.orange.com</tns:NTPServer1>
        <tns:NTPServer2>thotfr.orange.com</tns:NTPServer2>
        <tns:NTPServer3></tns:NTPServer3>
        <tns:NTPServer4></tns:NTPServer4>
        <tns:NTPServer5></tns:NTPServer5>
      </tns:NTP>
      <tns:CWMP>
        <tns:CWMPUsername></tns:CWMPUsername>
        <tns:CWMPPassword></tns:CWMPPassword>
        <tns:CWMPUrl>https://livebox.fr.karma.orange.com:443/krmx69/fr</tns:CWMPUrl>
        <tns:CWMPConReqUsername>$(ACS_USER)</tns:CWMPConReqUsername>
        <tns:CWMPConReqPassword>orange</tns:CWMPConReqPassword>
      </tns:CWMP>
      <tns:Timezone>Paris</tns:Timezone>
   </tns:AdminSettings>
   <tns:UI>
      <tns:DisplayConnectedDevicesOnHomePage>true</tns:DisplayConnectedDevicesOnHomePage>
   </tns:UI>
</tns:hgwconfig>
