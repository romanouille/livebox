         <tns:Sunday/>
         <tns:SundayStart/>
         <tns:SundayEnd/>
         <tns:SundayInvert/>
         <tns:Everyday/>
         <tns:EverydayStart/>
         <tns:EverydayEnd/>
         <tns:EverydayInvert/>
      </tns:WifiScheduling>
      <tns:DHCP>
         <tns:startaddress/>
         <tns:endaddress/>
         <tns:StaticAddress>
            <tns:MacAddress/>
            <tns:IPAddress/>
         </tns:StaticAddress>
         <tns:DHCPEnable/>
      </tns:DHCP>
      <tns:DHCPv6>
          <tns:DHCPv6Enable/>
      </tns:DHCPv6>
      <tns:DNS>
         <tns:mode/>
      </tns:DNS>
      <tns:DMZ>
         <tns:Mode/>
         <tns:StaticIP/>
      </tns:DMZ>
      <tns:NAPT>
         <tns:Name/>
         <tns:ExternalPort/>
         <tns:InternalPort/>
         <tns:Protocol/>
         <tns:IPAddress/>
         <tns:Enabled/>
      </tns:NAPT>
      <tns:DynDNS>
         <tns:Service/>
         <tns:FullHostName/>
         <tns:Username/>
         <tns:Password/>
         <tns:Enable/>
      </tns:DynDNS>
      <tns:Firewall>
         <tns:FirewallLevel/>
         <tns:FirewallRespondToPing/>
         <tns:FirewallRule>
            <tns:Service/>
            <tns:Protocol/>
            <tns:SourceAddress/>
            <tns:SourceMask/>
            <tns:SourcePort/>
            <tns:DestinationAddress/>
            <tns:DestinationMask/>
            <tns:DestinationPort/>
            <tns:Action/>
            <tns:Enable/>
         </tns:FirewallRule>
      </tns:Firewall>
