               <tns:Mode/>
               <tns:MACListEntry/>
            </tns:MACfiltering>
         </tns:SSID>
         <tns:Channel/>
      </tns:WifiConnection>
      <tns:WifiScheduling>
         <tns:AlwaysAllow/>
         <tns:Monday/>
         <tns:MondayStart/>
         <tns:MondayEnd/>
         <tns:MondayInvert/>
         <tns:Tuesday/>
