            <tns:DeviceType/>
            <tns:Icon/>
            <tns:Room/>
            <tns:BlackListDevice/>
            <tns:HideDevice/>
            <tns:Tags/>
            <tns:LastConnection/>
            <tns:InternetAccessRestrictions>
               <tns:AlwaysAllow/>
               <tns:Monday/>
               <tns:MondayStart/>
               <tns:MondayEnd/>
               <tns:Tuesday/>
               <tns:TuesdayStart/>
               <tns:TuesdayEnd/>
               <tns:Wednesday/>
               <tns:WednesdayStart/>
               <tns:WednesdayEnd/>
               <tns:Thursday/>
               <tns:ThursdayStart/>
               <tns:ThursdayEnd/>
               <tns:Friday/>
               <tns:FridayStart/>
               <tns:FridayEnd/>
               <tns:Saturday/>
               <tns:SaturdayStart/>
               <tns:SaturdayEnd/>
               <tns:Sunday/>
               <tns:SundayStart/>
               <tns:SundayEnd/>
               <tns:Everyday/>
               <tns:EverydayStart/>
               <tns:EverydayEnd/>
            </tns:InternetAccessRestrictions>
            <tns:Schedule>
              <tns:ID/>
              <tns:Enable/>
              <tns:Base/>
              <tns:Default/>
              <tns:Override/>
              <tns:Entries>
                 <tns:Entry>
                    <tns:State/>
                    <tns:Begin/>
                    <tns:End/>
                 </tns:Entry>
              </tns:Entries>
            </tns:Schedule>
            <tns:NameEntries>
               <tns:NameEntry>
                  <tns:Name/>
                  <tns:Source/>
               </tns:NameEntry>
            </tns:NameEntries>
         </tns:DeviceSettings>
      </tns:HomeNetworkMap>
   </tns:UI>
   <tns:ACSSettings>
      <tns:IGDTime>
         <tns:Enable/>
         <tns:LocalTimeZone/>
         <tns:LocalTimeZoneName/>
      </tns:IGDTime>
      <tns:IGDServices>
         <tns:OrangeCom_OrangeTV>
            <tns:Enable/>
         </tns:OrangeCom_OrangeTV>
         <tns:OrangeCom_WifiComm>
            <tns:Activate/>
            <tns:Enable/>
         </tns:OrangeCom_WifiComm>
      </tns:IGDServices>
   </tns:ACSSettings>
   <tns:PhoneBook>
      <tns:Entry>
         <tns:LastRevision/>
         <tns:UniqueID/>
