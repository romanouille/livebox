<div id="firewallCustomRule3" xmlns="http://www.orange.com/xhtml/engine/loose">
	<div class="boxType3">
	   <div id="helpPage">
			<a  id="help" href="javascript:;">
			<img align="middle" i18n="img.data.ico_txt_help" widgetType="Image"/>
			</a>
		</div>
		<h1 class="logo_param" i18n="page.myNetwork.firewallCustomRule3.title"/>
		<div class="margin-left-40 margin-top-m8">
			<strong  i18n="page.myNetwork.firewallCustomRule3.subtitle"/>
		</div>
	</div>
	<div class="maindiv">		
		<form action="#">
		  <div class="boxType4 border-bottom-3 border-top-1 margin-top-10">
			<div class="firewallCustomRuleLeft">
			<span class="h3Text" i18n="page.myNetwork.firewallCustomRule3.accesscontrol"/>
				<div class="status-block left">					
					<div class="clear"/>
					<div widgetType="FormInput" i18n="page.myNetwork.firewallCustomRule3.allow">
					<!--<input id="allow" type="checkbox" i18nPrefix="page.myNetwork.firewallCustomRule3.allow"/>-->
					<span  id="allow" class="radioBR"  widgetType="EnumInput" widgetArg="'radio'" enumerated="[true, false]"    i18nPrefix="page.myNetwork.firewallCustomRule3.allow"/>
					</div>
					<div widgetType="FormInput" i18n="page.myNetwork.firewallCustomRule3.activate">
					 <!--	<input id="activate" type="checkbox" dataId="Enable" i18nPrefix="page.myNetwork.firewallCustomRule3.activate"/>-->
						<span  id="activate" class="radioBR"  widgetType="EnumInput" widgetArg="'radio'" enumerated="[true, false]"   dataId="Enable"  i18nPrefix="page.myNetwork.firewallCustomRule3.activate"/>
					</div>
					
					<input style="display:none" id="Description" type="text" dataId="Description"/>
					<input style="display:none" id="Protocol" type="text" dataId="Protocol"/>
					<input style="display:none" id="SourceIP" type="text" dataId="SourceIP"/>
					<input style="display:none" id="SourceMask" type="text" dataId="SourceMask"/>
					<input style="display:none" id="SourcePort" type="text" dataId="SourcePort"/>
					<input style="display:none" id="SourcePortRangeEnd" type="text" dataId="SourcePortRangeEnd"/>
					<input style="display:none" id="DestinationIP" type="text" dataId="DestinationIP"/>
					<input style="display:none" id="DestinationMask" type="text" dataId="DestinationMask"/>
					<input style="display:none" id="DestinationPort" type="text" dataId="DestinationPort"/>
					<input style="display:none" id="DestinationPortRangeEnd" type="text" dataId="DestinationPortRangeEnd"/>
					<input style="display:none" id="Order" type="text" dataId="Order"/>
					<input style="display:none" id="Target" type="text" dataId="Target"/>
				</div>
			</div>

			<div class="firewallCustomRuleRight">
				<div class="localnetwork" i18n="page.myNetwork.firewallCustomRule1.connectedservice.localnetwork"/>
				<div class="application">&#160;</div>
				<div class="internet" i18n="page.myNetwork.firewallCustomRule1.connectedservice.internet"/>
				<div class="address">
					<span i18n="page.myNetwork.firewallCustomRule2.address.from"/>&#160;<span id="from"/><br/>
					<span i18n="page.myNetwork.firewallCustomRule2.address.port"/>&#160;<span id="port"/>
				</div>
			</div>
			<div class="clear"/>
          </div>
			<div widgetType="ButtonContainer" class="right" style="width: 100%">
				<div class="btnPageBack" >
				<span id="btnBack" widgetType="ButtonImage" i18n="page.myNetwork.firewallCustomRule2.back"/>
				</div>
				<div style="float : right">					
					<span id="btnPreviousStep" widgetType="ButtonImage" i18n="common.previous.step"/>
					<span widgetType="Button" widgetArg="{type: 'submit', crud: 'create'}" dataId="Device/Firewall/Rules/Rule" i18n="page.myNetwork.firewallCustomRule3.add"/>
				</div>
			</div>
		</form>
	</div>
</div>