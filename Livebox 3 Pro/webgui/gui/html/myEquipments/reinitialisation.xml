<div id="reinitialisation" xmlns="http://www.orange.com/xhtml/engine/loose">
	<span style="display:none">XpathRouter</span>
	<span style="display:none" id="rpcpath">XPathRPC</span>
	<input style="display:none" type="text" id="memoryCardStatus" dataId="XpathRouter/SmartCard/Status"/>
	<div class="boxType3">
	   <div id="helpPage">
			<a  id="help" href="javascript:;">
			<img align="middle" i18n="img.data.ico_txt_help" widgetType="Image"/>
			</a>
		</div>
	  	<h1 class="logo_EnvLiveBox_reinitialisation">
			<span i18n="page.myEquipments.reinitialisation.title"/>&#160;<span dataId="XpathRouter/RouterName"/>
	 	</h1>
		 <div class="margin-left-40 margin-top-m8">
		 	<strong i18n="page.myEquipments.reinitialisation.subtitle"/>
		 </div>
	</div>
	<div class="maindiv">		
		<form>
		<div class="boxType4 margin-top-10 border-bottom-3">
			<span class="h3Text margin-bottom-10" i18n="page.myEquipments.reinitialisation.reinit"/>
			<br/>
			<div  i18n="page.myEquipments.reinitialisation.reinit.labelMaster"/><!-- class="forminput" -->
			<div id="insertCard">
				<div class=" bold margin-bottom-10" i18n="page.myEquipments.reinitialisation.reinit.labelMaster2"/><!-- class="notificationSmall" -->
			</div>
			<div class="forminput">
				<span id="radioType" class="radioBR" widgetType="EnumInput" widgetArg="'radio'" enumerated="['0', '1', '2']" i18nPrefix="page.myEquipments.reinitialisation.reinit.type"/>
				<div class="clear"/>
			</div>
			<div class="notificationSmall" i18n="page.myEquipments.reinitialisation.reinit.typedescription.2.notification"/>
			
			<div widgetType="ButtonContainer" class="right">
				<span id="btnReinitBox" widgetType="ButtonImage" i18n="page.myEquipments.reinitialisation.btn"/>
			</div>		
			<div  i18n="page.myEquipments.reinitialisation.reinit.label3"/>	<!-- class="forminput" -->
		</div>					
		<!-- <div widgetType="ButtonContainer" class="btnPageBack">
			<span widgetType="MenuItem,ButtonImage" widgetArg="{MenuItem: 'PREVIOUSPAGE'}" i18n="common.page.back"/>
		</div> -->
	  </form>
	</div>
</div>